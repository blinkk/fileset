{
  "name": "@blinkk/fileset",
  "bin": {
    "fileset": "./dist/src/index.js"
  },
  "version": "0.1.1",
  "license": "MIT",
  "homepage": "https://github.com/blinkkcode/fileset/",
  "repository": {
    "type": "git",
    "url": "git://github.com/blinkkcode/fileset.git"
  },
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "copy": "cpx 'src/static/**' 'dist/src/static'",
    "dev:start": "npm run copy; npm run compile",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "docs": "typedoc --out docs src/",
    "fix": "node ./node_modules/eslint/bin/eslint --fix src/**/*.ts src/**/*.tsx",
    "test": "ava"
  },
  "ava": {
    "require": [
      "ts-node/register"
    ],
    "extensions": [
      "ts"
    ],
    "files": [
      "src/*.test.ts"
    ]
  },
  "devDependencies": {
    "@types/async": "^3.2.5",
    "@types/cli-progress": "^3.8.0",
    "@types/express": "^4.17.9",
    "@types/http-proxy": "^1.17.4",
    "@types/js-yaml": "^3.12.5",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^14.14.16",
    "ava": "^3.14.0",
    "cpx": "^1.5.0",
    "gts": "^3.0.3",
    "nodemon": "^2.0.6",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@google-cloud/datastore": "^6.3.1",
    "@google-cloud/storage": "^5.7.0",
    "async": "^3.2.0",
    "cli-progress": "^3.8.2",
    "commander": "^6.2.1",
    "express": "^4.17.1",
    "fast-crc32c": "^2.0.0",
    "google-auth-library": "^6.1.3",
    "http-proxy": "^1.18.1",
    "isomorphic-git": "^1.8.0",
    "js-yaml": "^3.14.1",
    "mime-types": "^2.1.27"
  },
  "files": [
    "dist/src"
  ]
}
